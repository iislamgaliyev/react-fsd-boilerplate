    "react-dom": "18.2.0",
    "react-i18next": "^14.0.1",
    "react-if": "4.1.5",
    "react-router-dom": "6.15.0",
    "react-toastify": "8.0.3",
    "zustand": "4.4.1"
  },
  "devDependencies": {
